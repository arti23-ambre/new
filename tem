package Add;

public class TemperatureConverter {
	static float Celsiustofahrenheit(float c) {
		return (c*9/5)+32;
		
	}
	static double fahrenhitetocelsius(double f) {
		return((f-32)*5/9);
		
	}

	public static void main(String[] args) {
		float c = 34;
		double f = 65;
		System.out.println("c :" + Celsiustofahrenheit(c)+"f");
		System.out.println("f:" + fahrenhitetocelsius(f)+"c");
	

	}

}
